@page
@model BikeStore.Pages.CustomersModel
@{
    ViewData["Title"] = "Clienti";
}


<div class="py-5 text-center">
    <img class="d-block mx-auto mb-4" src="data:image/png;base64," alt="" width="72" height="72">
    <h2>Clienti</h2>
    <p class="lead">[descrizione]</p>
</div>


    
<div class="row">
<form class="col-sm-6 card card-sm" method="post" asp-page-handler = "Ricerca">
    <div class="card-body row no-gutters align-items-center">
        <div class="col-auto">
            <i class="fas fa-search h4 text-body"></i>
        </div>
        <!--end of col-->
        <div class="col">
            <input class="form-control form-control-lg form-control-borderless" type="search" placeholder="Cerca cliente" asp-for="@Model.SrcParametro">
        </div>
        <!--end of col-->
        <div class="col-auto">
            <button class="btn btn-lg btn-primary" type="submit">
                <i class="bi bi-search"></i>
            </button>
        </div>
        <!--end of col-->
    </div>
</form>
<div class="col-sm-3"></div>
<div class="col-sm-3">
        <a style="width:100%;
              height: 100%;
              display: flex;
              align-items: center;
              justify-content: center;" class=" col-sm-3" asp-page-handler="AddNew">
            <button class="btn btn-lg btn-primary" type="submit">
                Aggiungi nuovo
                <i class="bi bi-person-fill-add"></i>
            </button>
        </a>
</div>

</div>

<br><br>

<td class="px-4">

        @if(Model.Customers.Count > 0)
        {
            <td class="table-wrapper">
                <!--Table-->
                <table class="table table-hover mb-0">

                    <!--Table head-->
                    <thead>
                        <tr>
                            <th class="th-lg">
                                <a>
                                    Nome
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Email
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Telefono
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Indirizzo
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Città
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Zipcode
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                        </tr>
                    </thead>
                    <!--Table head-->
                    <!--Table body-->
                    <tbody>
                        @foreach (var item in Model.Customers)
                        {
                            <tr>
                                @{
                                    string s = item.FirstName + " " + item.LastName;
                                }
                                <td>@item.FirstName @item.LastName</td>
                                <td>@item.Email</td>
                                <td>@item.Phone</td>
                                <td>@item.Street</td>
                                <td>@item.City, @item.State</td>
                                <td>@item.ZipCode</td>
                                <td class="col-auto" >
                                    <a asp-page-handler="Delete" asp-route-id="@item.Id">
                                        <button class="btn btn-lg btn-danger" type="submit">
                                            <i class="bi bi-trash-fill"></i>
                                        </button>
                                    </a>                                    
                                </td>
                                <td class="col-auto">
                            <a asp-page-handler="Edit" asp-route-id="@item.Id">
                                        <button class="btn btn-lg btn-primary" type="submit">
                                                    <i class="bi bi-pencil-square"></i>
                                        </button>
                                    </a>                                    
                                </td>
                            </tr>
                            
                        }

                    </tbody>
                    <!--Table body-->
                </table>
                <!--Table-->
            </td>
        }
        else
        {
            <div class="py-5 text-center">I dati non soddisfano i criteri di ricerca</div>
        }


